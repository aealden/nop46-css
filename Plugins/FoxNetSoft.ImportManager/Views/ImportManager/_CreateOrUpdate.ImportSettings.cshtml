                    <span asp-validation-for="RemoveTags"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveRelatedProduct">
                <div class="col-md-3">
                    <nop-label asp-for="RemoveRelatedProduct" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveRelatedProduct" />
                    <span asp-validation-for="RemoveRelatedProduct"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveCrossSellProduct">
                <div class="col-md-3">
                    <nop-label asp-for="RemoveCrossSellProduct" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveCrossSellProduct" />
                    <span asp-validation-for="RemoveCrossSellProduct"></span>
