                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveProductSpecificationOptions" />
                    <span asp-validation-for="RemoveProductSpecificationOptions"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemovePictures">
                <div class="col-md-3">
                    <nop-label asp-for="RemovePictures" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemovePictures" />
                    <span asp-validation-for="RemovePictures"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveAssociatedPicture">
                <div class="col-md-3">
                    <nop-label asp-for="RemoveAssociatedPicture" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveAssociatedPicture" />
                    <span asp-validation-for="RemoveAssociatedPicture"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveCategories">
                <div class="col-md-3">
                    <nop-label asp-for="RemoveCategories" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveCategories" />
                    <span asp-validation-for="RemoveCategories"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveManufacturers">
                <div class="col-md-3">
                    <nop-label asp-for="RemoveManufacturers" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveManufacturers" />
                    <span asp-validation-for="RemoveManufacturers"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveTags">
                <div class="col-md-3">
                    <nop-label asp-for="RemoveTags" />
                </div>
                <div class="col-md-9">
