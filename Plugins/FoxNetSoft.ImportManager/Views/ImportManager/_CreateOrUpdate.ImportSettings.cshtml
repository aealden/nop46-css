@model FoxNetSoft.Plugin.Feed.ImportManager.Models.ImportTemplateModel
<div class="cards-group">
    <div class="card card-default">
        <div class="card-body">
            <div class="form-group row" id="pnSkipFirstLine">
                <div class="col-md-3">
                    <nop-label asp-for="SkipFirstLine" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="SkipFirstLine" />
                    <span asp-validation-for="SkipFirstLine"></span>
                </div>
            </div>
            <div class="form-group row" id="pnColumnDelimiter">
                <div class="col-md-3">
                    <nop-label asp-for="ColumnDelimiter" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="ColumnDelimiter" />
                    <span asp-validation-for="ColumnDelimiter"></span>
                </div>
            </div>
            <div class="form-group row" id="pnFieldEnclosure">
                <div class="col-md-3">
                    <nop-label asp-for="FieldEnclosure" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="FieldEnclosure" />
                    <span asp-validation-for="FieldEnclosure"></span>
                </div>
            </div>
            <div class="form-group row">
                <div class="col-md-3">
                    <nop-label asp-for="DateTimeFormat" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="DateTimeFormat" />
                    <span asp-validation-for="DateTimeFormat"></span>
                </div>
            </div>
            <div class="form-group row">
                <div class="col-md-3">
                    <nop-label asp-for="DecimalDelimiterId" />
                </div>
                <div class="col-md-9">
                    <nop-select asp-for="DecimalDelimiterId" asp-items="Model.AvailableDecimalDelimiters" />
                    <span asp-validation-for="DecimalDelimiterId"></span>
                </div>
            </div>
        </div>
    </div>
    <div class="card card-default productentity">
        <div class="card-body">
            <div class="form-group row" @(Model.AvailableWarehouses.SelectionIsNotPossible() ? Html.Raw("style=\"display:none\"") : null)>
                <div class="col-md-3">
                    <nop-label asp-for="WarehouseId" />
                </div>
                <div class="col-md-9">
                    <nop-select asp-for="WarehouseId" asp-items="Model.AvailableWarehouses" />
                    <span asp-validation-for="WarehouseId"></span>
                </div>
            </div>
            <div class="form-group row" @(Model.AvailableManufacturers.SelectionIsNotPossible() ? Html.Raw("style=\"display:none\"") : null)>
                <div class="col-md-3">
                    <nop-label asp-for="ManufacturerId" />
                </div>
                <div class="col-md-9">
                    <nop-select asp-for="ManufacturerId" asp-items="Model.AvailableManufacturers" />
                    <span asp-validation-for="ManufacturerId"></span>
                </div>
            </div>
            <div class="form-group row" @(Model.AvailableVendors.SelectionIsNotPossible() ? Html.Raw("style=\"display:none\"") : null)>
                <div class="col-md-3">
                    <nop-label asp-for="VendorId" />
                </div>
                <div class="col-md-9">
                    <nop-select asp-for="VendorId" asp-items="Model.AvailableVendors" />
                    <span asp-validation-for="VendorId"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveTierPrices">
                <div class="col-md-3">
                    <nop-label asp-for="RemoveTierPrices" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveTierPrices" />
                    <span asp-validation-for="RemoveTierPrices"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveProductAttributes">
                <div class="col-md-3">
                    <nop-label asp-for="RemoveProductAttributes" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveProductAttributes" />
                    <span asp-validation-for="RemoveProductAttributes"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveProductAttributeCombinations">
                <div class="col-md-3">
                    <nop-label asp-for="RemoveProductAttributeCombinations" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveProductAttributeCombinations" />
                    <span asp-validation-for="RemoveProductAttributeCombinations"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveProductAttributeValues">
