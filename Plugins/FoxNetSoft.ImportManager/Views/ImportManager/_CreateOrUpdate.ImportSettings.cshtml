                    <nop-label asp-for="RemoveProductAttributeValues" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveProductAttributeValues" />
                    <span asp-validation-for="RemoveProductAttributeValues"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveProductSpecifications">
                <div class="col-md-3">
                    <nop-label asp-for="RemoveProductSpecifications" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="RemoveProductSpecifications" />
                    <span asp-validation-for="RemoveProductSpecifications"></span>
                </div>
            </div>
            <div class="form-group row removeoldvalues" id="pnRemoveProductSpecificationOptions">
