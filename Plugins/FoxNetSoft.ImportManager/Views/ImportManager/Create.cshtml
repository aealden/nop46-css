@model FoxNetSoft.Plugin.Feed.ImportManager.Models.ImportTemplateModel
@using Nop.Web.Framework.UI
@{
    Layout = "_AdminLayout";
    //page title
    ViewBag.PageTitle = @T("Admin.FoxNetSoft.Plugin.Feed.ImportManager.ImportTemplate.AddNew").Text;

    NopHtml.SetActiveMenuItemSystemName("FoxNetSoft.ImportManager.List");
}
<form asp-controller="ImportManager" asp-action="Create" method="post" id="importmanager-form" enctype="multipart/form-data">
    <div class="content-header clearfix">
        <h1 class="float-left">
            @T("Admin.FoxNetSoft.Plugin.Feed.ImportManager.ImportTemplate.AddNew")
            <small>
                <i class="fa fa-arrow-circle-left"></i>
                <a asp-action="List">@T("Admin.FoxNetSoft.Plugin.Feed.ImportManager.ImportTemplate.BackToList")</a>
            </small>
        </h1>
        <div class="float-right">
            <button type="submit" name="save" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.Save")
            </button>
            <button type="submit" name="save-continue" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.SaveContinue")
            </button>
        </div>
    </div>
    @await Html.PartialAsync("~/Plugins/FoxNetSoft.ImportManager/Views/ImportManager/_CreateOrUpdate.cshtml", Model)
</form>

